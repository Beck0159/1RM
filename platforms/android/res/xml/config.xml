<?xml version='1.0' encoding='utf-8'?>
<widget id="com.beck0159.repCalculator" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <preference name="AndroidLaunchMode" value="singleTop" />
    <feature name="App">
        <param name="android-package" value="org.apache.cordova.App" />
    </feature>
    <feature name="EmailComposer">
        <param name="android-package" value="de.appplant.cordova.emailcomposer.EmailComposer" />
    </feature>
    <feature name="SplashScreen">
        <param name="android-package" value="org.apache.cordova.splashscreen.SplashScreen" />
    </feature>
    <name>Lift Calculator</name>
    <description>
        A application to calculate one rep maxes and more.
    </description>
    <author email="connorbecker@yahoo.ca" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <preference name="SplashScreen" value="screen" />
    <preference name="SplashScreenDelay" value="1000" />
    <content src="index.html" />
    <access origin="*" />
</widget>
